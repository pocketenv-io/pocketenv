        ],
        "properties": {
          "id": {
            "type": "string",
            "description": "The sandbox ID or URI to retrieve"
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "ref",
          "ref": "io.pocketenv.sandbox.defs#sandboxViewDetailed"
        }
      }
    }
  }
}
